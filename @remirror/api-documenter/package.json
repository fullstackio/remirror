{
  "name": "@remirror/api-documenter",
  "description": "Read JSON files from api-extractor, generate documentation pages",
  "version": "0.0.1-alpha.11",
  "license": "MIT",
  "bin": {
    "api-documenter": "./bin/api-documenter"
  },
  "dependencies": {
    "@microsoft/api-extractor-model": "7.0.28",
    "@microsoft/node-core-library": "3.13.0",
    "@microsoft/ts-command-line": "4.2.3",
    "@microsoft/tsdoc": "0.12.9",
    "colors": "1.3.3",
    "lodash.kebabcase": "4.1.1"
  },
  "devDependencies": {
    "@microsoft/node-library-build": "6.0.55",
    "@microsoft/rush-stack-compiler-3.2": "0.3.7",
    "@types/lodash.kebabcase": "4.1.6"
  },
  "homepage": "https://api-extractor.com/",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/Microsoft/web-build-tools/tree/master/apps/api-documenter"
  },
  "scripts": {
    "build": "run-p build:*",
    "_build:babel": "babel src --out-dir lib --extensions \".ts,.tsx\" -s",
    "build:declaration": "tsc -p ./tsconfig.prod.json  --declarationMap",
    "dev": "run-p dev:*",
    "_dev:babel": "babel src --out-dir lib --extensions \".ts,.tsx\" -s --watch",
    "dev:declaration": "tsc -p ./tsconfig.prod.json --declarationMap --watch",
    "lint": "tslint --project tsconfig.json --config ../../tslint.json",
    "typecheck": "tsc -p ./tsconfig.json --noEmit"
  }
}
