  "repository": "https://github.com/ifiokjr/remirror/tree/apps/shared",
  "version": "0.0.0",
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "_build": "run-s build:babel",
    "_lint": "tslint --project tsconfig.json --config ../../tslint.json",
    "_typecheck": "tsc -p ./tsconfig.json --noEmit",
    "build:babel": "babel src --out-dir lib --extensions \".ts\" --source-maps true",
    "clean": "rimraf lib secret.json",
    "copy:db": "cp ../support/secrets/db.json db.json",
    "copy:secret": "cp ../support/secrets/key.json secret.json",
    "dev": "run-p watch:*",
    "watch:babel": "babel src --out-dir lib --extensions \".ts\" --watch"
  },
  "dependencies": {
    "@schemafire/core": "file:/Users/ifiokjr/Coding/kickjump/schemafire/@schemafire/core",
    "@schemafire/firestore": "file:/Users/ifiokjr/Coding/kickjump/schemafire/@schemafire/firestore",
    "express": "4.16.4",
    "firebase-admin": "7.3.0",
    "firebase-functions": "2.3.1",
    "glob": "7.1.4",
    "got": "9.6.0",
    "io-ts": "1.8.5",
    "lodash": "4.17.11"
  },
  "engines": {
    "node": "8"
  }
}
