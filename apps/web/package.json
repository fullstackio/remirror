{
  "private": true,
  "name": "web",
  "version": "0.0.0",
  "main": "./dist/server/index.js",
  "browser": {
    "firebase-admin": false
  },
  "scripts": {
    "_build": "run-s build:*",
    "_lint": "run-s \"lint:* {1}\" --",
    "_typecheck": "run-s \"typecheck:* {1}\" --",
    "analyze": "cross-env BUNDLE_ANALYZE=browser next build",
    "build:next": "next build",
    "build:server": "babel . --out-dir dist --extensions .ts --only server,utils,routes,start.ts,typings,workers --ignore typings/globals.d.ts **/__tests__ -s",
    "clean": "rimraf .next dist",
    "deploy": "run-s lockfile:add build deploy:cmd lockfile:remove",
    "deploy:cmd": "echo not yet implemented",
    "dev": "cross-env RESOLVE_TS=true run-p watch:* nodemon",
    "lint:next": "tslint --project tsconfig.json --config tslint.json",
    "lint:server": "tslint --project tsconfig.server.json --config tslint.json",
    "lockfile:add": "cp -p ../../yarn.lock yarn.lock",
    "lockfile:remove": "rimraf yarn.lock",
    "nodemon": "nodemon",
    "start": "cross-env NODE_ENV=production node dist/start.js",
    "typecheck:next": "tsc -p tsconfig.json",
    "typecheck:server": "tsc -p tsconfig.server.json",
    "typecheck:workers": "tsc -p workers/tsconfig.json",
    "watch:server": "babel . --out-dir dist --extensions .ts --only server,utils,routes,start.ts,typings,workers -s --watch"
  },
